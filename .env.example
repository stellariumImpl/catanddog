# Postgres connection string
# Use Supabase Session Pooler for runtime (IPv4 compatible)
DATABASE_URL="postgresql://postgres:YOUR_PASSWORD@YOUR_POOLER_HOST:5432/postgres?pgbouncer=true&connection_limit=1&sslmode=require"
# Use Direct connection only for Prisma migrations
DIRECT_URL="postgresql://postgres:YOUR_PASSWORD@YOUR_DIRECT_HOST:5432/postgres?sslmode=require"
